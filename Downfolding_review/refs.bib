@Article{Pati_Rao,
author ="Pati, Swapan K. and Rao, C. N. R.",
title  ="Kagome network compounds and their novel magnetic properties",
journal  ="Chem. Commun.",
year  ="2008",
issue  ="39",
pages  ="4683-4693",
publisher  ="The Royal Society of Chemistry",
abstract  ="Compounds possessing the Kagome network are truly interesting because of their unusual low-energy properties. They exhibit magnetic frustration because of the triangular lattice inherent to the hexagonal bronze structure they possess{,} as indeed demonstrated by some of the Fe3+ jarosites{,} but this is not the general case. Kagome compounds formed by transition metal ions with varying spins exhibit novel magnetic properties{,} some even showing evidence for magnetic order and absence of frustration. We describe the structure and magnetic properties of this interesting class of materials and attempt to provide an explanation for the variety of properties on the basis of theoretical considerations."}

