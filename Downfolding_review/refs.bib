@article{Husimi_SL,
  author={P Chandra and B Doucot},
  title={Spin liquids on the Husimi cactus},
  journal={Journal of Physics A: Mathematical and General},
  volume={27},
  number={5},
  pages={1541},
  year={1994},
  abstract={We have studied the failure of 'fluctuation-selection' in a ground state manifold associated with an extensive entropy. The recursive structure of the Husimi cactus permits exact calculation, and thus avoids problems associated with non-perturbative fluctuations. The structure of the barriers separating the ground states can be determined; in all the cases we have examined, we find that the system prefers to remain a spin liquid rather than to 'localize' in an ordered state that breaks the degeneracy of the manifold.}
}

